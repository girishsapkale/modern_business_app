    <!-- Page Content -->
    <div class="container">

        <!-- Sign Up Form -->
        <!-- In order to set the email address and subject line for the contact form go to the bin/contact_me.php file. -->
        <div class="row">
            <div class="col-md-8">
                <h3>Sign Up</h3>
                <!-- <form name="sentMessage" id="contactForm" novalidate> -->
                <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                <%= devise_error_messages! %>

                    <div class="control-group form-group">
                        <div class="controls">
                            <label>Name:</label>
                           
                            <%= f.text_field :name, :id=>"Name", :class=>"form-control", :required => true %>
                             
                            <!-- <input type="email" class="form-control" id="email" required data-validation-required-message="Please enter your email address."> -->
                        </div>
                    </div>

                    <div class="control-group form-group">
                        <div class="controls">
                            <label>Email:</label>
                           
                            <%= f.email_field :email, :id=>"email", :class=>"form-control", :required => true %>
                             
                            <!-- <input type="email" class="form-control" id="email" required data-validation-required-message="Please enter your email address."> -->
                        </div>
                    </div>

                    <div class="control-group form-group">
                        <div class="controls">
                            <label>Phone number:</label>
                            <%= f.text_field :phone, :class=>"form-control", :required => true %>
                            
                           <!--  <input type="tel" class="form-control" id="phone" required data-validation-required-message="Please enter your phone number."> -->
                        </div>
                    </div>
                  

                    <div class="control-group form-group">
                        <div class="controls">
                            <label>Password:</label>
                            <%= f.text_field :password, :type => "password", :class=>"form-control", :required => true%>
                            
                           <!--  <input type="tel" class="form-control" id="phone" required data-validation-required-message="Please enter your phone number."> -->
                        </div>
                    </div>
                    
                    <div class="control-group form-group">
                        <div class="controls">
                            <label>Confirm Password:</label>
                            <%= f.text_field :password_confirmation, :type => "password", :class=>"form-control", :required => true%>
                            
                           <!--  <input type="tel" class="form-control" id="phone" required data-validation-required-message="Please enter your phone number."> -->
                        </div>
                    </div>


                    <div id="success"></div>
                    <!-- For success/fail messages -->
                    <button type="submit" class="btn btn-primary">Sign Up</button>

                <% end %>
                
                <%= render "devise/shared/links" %>
            </div>

        </div>
        <!-- /.row -->

      
    </div>
